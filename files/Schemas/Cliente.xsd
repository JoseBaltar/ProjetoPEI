<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="ProjetoPEI/Grupo/Entrega1/Cliente"
           xmlns="ProjetoPEI/Grupo/Entrega1/Cliente"
           
           xmlns:cmn="ProjetoPEI/Grupo/Entrega1/CommonSchema"
           elementFormDefault="qualified">
    
    <xs:import schemaLocation="CommonSchema.xsd" 
               namespace="ProjetoPEI/Grupo/Entrega1/CommonSchema"/>
			
        <xs:complexType name="typeClient">
            <xs:sequence>
                <xs:element name="nome" type="cmn:typeNome"/>
                <xs:element name="telefone" type="cmn:typeTelefone"/>
                <xs:element name="email" type="cmn:typeEmail"/>
                <xs:element name="NIF" type="cmn:NIF"/>
            </xs:sequence>
        </xs:complexType>
  
	
</xs:schema>